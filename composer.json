{
    "name": "kepson/dexchange-php",
    "description": "dexchange api sdk for php",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dexchange\\Client\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kepsondiaz",
            "email": "kherabadiaby328@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "symfony/options-resolver": "^6.4",
        "php-http/discovery": "^1.12",
        "guzzlehttp/guzzle": "^7.9",
        "php-http/client-common": "^2.3",
        "php-http/guzzle7-adapter": "^v1.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "pestphp/pest": "1.x-dev",
        "mockery/mockery": "2.0.x-dev"
    }
}
